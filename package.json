    "swagger",
    "api"
  ],
  "version": "7.0.0",
  "main": "lib/jsonApi.js",
  "types": "types/jsonApi.d.ts",
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/EkkoSense/jagql-framework"
  },
  "engines": {
    "node": ">=8",
    "npm": ">=5",
    "yarn": ">=1"
  },
  "dependencies": {
    "@types/express": "^4.11.1",
    "@types/joi": "^14.0.0",
    "async": "^3.2.3",
    "cookie-parser": "^1.4.7",
    "debug": "^4.1.0",
    "express": "^4.16.4",
    "joi": "^14.0.0",
    "joi-date-extensions": "^1.2.0",
    "lodash.assign": "4.2.0",
    "lodash.isequal": "4.5.0",
    "lodash.omit": "4.5.0",
    "lodash.uniq": "4.5.0",
