{
  "compilerOptions": {
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "ES2022",
    "noEmit": true
  },
  "files": ["types"],
  "exclude": [
    "node_modules",
    "lib"
  ],
  "typedocOptions": {
    "includes": "resources/guides",
    "out": "docs",
    "name": "jagql",
    "includeDeclarations": true,
    "excludeExternals": true,
    "mode": "modules"
  }
}
